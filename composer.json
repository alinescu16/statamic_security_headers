{
    "name": "alinandrei/security_headers",
    "description": "A Statamic addon to manage security headers.",
    "license": "proprietary",
    "autoload": {
        "psr-4": {
            "Alinandrei\\SecurityHeaders\\": "src"
        }
    },
    "require": {
        "statamic/cms": "^5.0"
    },
    "config": {
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": true
        }
    },
    "extra": {
        "statamic": {
            "name": "Security_Headers",
            "description": "Security_Headers addon"
        },
        "laravel": {
            "providers": [
                "Alinandrei\\SecurityHeaders\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
